    "@types/child-process-promise": "^2.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.3",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
